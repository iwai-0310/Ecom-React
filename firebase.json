{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/products.json",
        "destination": "/products.json"
      },    
      {
        "source": "**",
        "destination": "/index.html"
      }
      
    ]
  }
}
